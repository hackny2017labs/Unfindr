{
  "manifest_version": 2,
  "name": "Unfindr",
  "version": "0.1",
  "content_scripts": [
  	{
    	"matches": [
      		"https://m.facebook.com/friends/center/friends"
    	],
    	"js": [
        "content.js"
      ],
      "run_at": "document_end",

  	}
  ],
  "browser_action": {
  },
  "background": {
  	"scripts": ["background.js"]
  },
  "permissions": [
    "tabs"
  ]
}